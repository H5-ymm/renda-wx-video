
	<view class="detail-view">
		<view class="weui-flex between header-box">
			
  <view class="weui-flex around tabBar"  wx:if="{{!isSeachWidth}}">
    <block wx:for="{{$tabBar$tabBarList}}" wx:key="index">
      <view class="{{$tabBar$activeIndex==index?'tabBar_item-active':''}} weui-flex__item tabBar_item" bindtap="$tabBar$switchTab" data-wpyswitchtab-a="{{index}}">
        <view>{{item.name}}</view>
        <view wx:if="{{item.num!=-1}}">({{item.num}})</view>
      </view>
    </block>
    <view class="tab-line {{'line'+$tabBar$activeIndex}}"/>
  </view>

			
  <view class="weui-flex between search-box search-box {{isSeachWidth?'weui-flex__item':''}}">
		<view class="weui-cell weui-flex start weui-flex__item">
			<image src="../../../images/search.png" class="searchIcon"/>
			<input class="weui-input weui-flex__item search-input" bindinput="$search$inputBind" confirm-type="search" bindfocus="$search$inputFocus" value="{{$search$name}}" placeholder="搜索词"/>
    </view>
		<text>搜索</text>
	</view>

		</view>
		<view class="page_margin view-detail person-list">
			<scroll-view scroll-y="true" style="height:100%">
				
	<view class="resume-list">
		<block  wx:for="{{$teamResumeList$list}}" wx:key="index">
			<view class="page_card" wx:if="{{$teamResumeList$list.length}}">
				<view class="page_card_row weui-flex between">
					<view class="weui-flex start">
						<view>
							<view class="page_card_name">{{item.name}}</view>
							<view class="text-line weui-flex__item">
								{{item.age}}岁 {{item.sex==1?'男':'女'}} {{item.education}}</view>
							<view class="text-line weui-flex__item">
								{{item.citys||(item.province?item.province:'-'+item.city?item.city:'')||'-'}}
							</view>
						</view>
					</view>
					<view class="weui-flex end wrap page_card_end">
						<view class="page_card_status-resume">
							<view class="page_card_receipt_status {{'page_card_receipt_status'+item.status}}" wx:if="{{$teamResumeList$receiptType==2}}">{{item.status==1?'已通过':item.status==2?'已拒绝':'待审核'}}</view>
							<view class="page_card_receipt_status {{'page_card_receipt_status'+item.interview_status}}" wx:if="{{$teamResumeList$receiptType==3}}">{{item.interview_status==1?'已通过':item.interview_status==2?'已拒绝':'待审核'}}</view>
							<view class="page_card_receipt_status {{'page_card_receipt_status'+item.entry_status}}" wx:if="{{$teamResumeList$receiptType==4}}">{{item.entry_status==1?'已通过':item.entry_status==2?'已拒绝':'待审核'}}</view>
						</view>
						<view class="page_card_time">{{item.addtime}}</view>
					</view>
				</view>
			</view>
		</block>
		<view  wx:if="{{!$teamResumeList$list.length}}" class="page_list">
			<image src="https://a.rsd123.com/image/images/noBg.png" class="page_noData" mode="scaleToFill"/>
			<view class="page_noData_text">暂无数据！</view>
		</view>
	</view>

			</scroll-view>
		</view>
		<view class="only-btn" wx:if="{{list.length}}">
			<view class="weui-flex center">
				<button class="weui-btn_cell only-btn-item {{!receiptTime?'weui-btn_cell-disabled':'weui-btn_primary'}}" disabled="{{!receiptTime}}" bindtap="viewNotice">
					{{receiptType==2?'查看面试通知':'查看入职通知'}}
				</button>
				<button class="weui-btn_cell only-btn-item weui-btn_primary_plain" bindtap="recommendResume">
					推荐简历
				</button>
			</view>
		</view>
	<!-- </view> -->
	 
	<view hidden="{{$viewNoticeModal$isScaleModal}}">
		<view bindtap="$viewNoticeModal$ handleClose"  wx:if="{{$viewNoticeModal$isShow}}" class="modal_mask"/>
		<view class="modal_box modal-notice_box modal-viewNotice_box transition3s {{ $viewNoticeModal$isShow ? 'g_scale1': ''}}" style="height:{{$viewNoticeModal$height}}rpx">
			<view class="modal-content">
				<view class="modal-title">查看{{$viewNoticeModal$title}}通知</view>
				<view class="modal-view-box">
					<view class="modal-row">
						<text>面试时间</text>
						<view class="modal-col">{{$viewNoticeModal$form.time}}</view>
					</view>
					<view class="modal-row">
						<text>面试地址</text>
						<view class="modal-col"> {{$viewNoticeModal$form.address}}</view>
						<view class="modal-col"> {{$viewNoticeModal$form.detailAddress||'-'}}</view>
					</view>
					<view class="modal-row">
						<text>通知内容</text>
						<view class="modal-col">
							<view class="weui-input weui-textarea">
								{{$viewNoticeModal$form.content}}
							</view>
						</view>
					</view>
				</view>
				<view class="weui-cell_btn weui-flex center">
					<button class="weui-btn_cell weui-btn_cell-gradient" bindtap="$viewNoticeModal$handleOk">我知道了</button>
				</view>
			</view>
			<image src="https://a.rsd123.com/image/images/close.png" bindtap="$viewNoticeModal$handleClose" class="close-icon"/>
		</view>
	</view>


	</view>