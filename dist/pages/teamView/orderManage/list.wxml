
	<view class="home-view">
		<view class="home-view-header"/>
		<view class="page page_margin">
			<scroll-view scroll-y="true" style="height:100%" bindscrolltolower="searchScrollLower">
				<view class="home-view-box">
					
  <view class="weui-flex between search-box weui-cell weui-flex start page_card">
		<view class="weui-cell weui-flex start weui-flex__item">
			<image src="../../../images/search.png" class="searchIcon"/>
			<input class="weui-input weui-flex__item search-input" bindinput="$search$inputBind" confirm-type="search" bindfocus="$search$inputFocus" value="{{$search$name}}" placeholder="搜索词"/>
    </view>
		<text>搜索</text>
	</view>

					
  <view>
    <view  wx:if="{{$teamReceiptList$list.length}}">
      <block wx:for="{{$teamReceiptList$list}}" wx:for-index="$teamReceiptList$index" wx:for-item="$teamReceiptList$rItem" wx:key="$teamReceiptList$index">
        <view class="page_card" bindtap="$teamReceiptList$viewDetail" data-wpyviewdetail-a="{{$teamReceiptList$rItem}}">
          <wxs src="../../../filterWxs/receiptFilter.wxs" module="wxs"/>

	<view class="page_card_row weui-flex between">
		<view class="weui-flex__item">
			<view class="weui-flex between">
				<view class="page_card_name">
					<view>{{$teamReceiptList$rItem.job_name}}</view>
					<view class="badgeNum badgeNum3"  wx:if="{{!$teamReceiptList$rItem.team_isread&&$teamReceiptList$teamReceiptItem$receiptType<2}}">
						New
					</view>
					<view  wx:if="{{$teamReceiptList$rItem.msg_num&&$teamReceiptList$teamReceiptItem$receiptType>1}}">
						<view class="badgeNum" wx:if="{{$teamReceiptList$rItem.msg_num<10}}">
							{{$teamReceiptList$rItem.msg_num}}
						</view>
						<view class="badgeNum badgeNum1" wx:if="{{$teamReceiptList$rItem.msg_num>10&&$teamReceiptList$rItem.msg_num<100}}">{{$teamReceiptList$rItem.msg_num}}</view>
						<view class="badgeNum badgeNum2" wx:if="{{$teamReceiptList$rItem.msg_num>10&&$teamReceiptList$rItem.msg_num>100}}">99+</view>
					</view>
				</view>
				<view wx:if="{{!$teamReceiptList$teamReceiptItem$receiptType}}">
					<view wx:if="{{$teamReceiptList$rItem.job_status==1}}" class="page_card_receipt_status {{'page_card_receipt_status'+$teamReceiptList$rItem.status}}">
						{{wxs.statusTeamText($teamReceiptList$rItem.status)}}
					</view>
					<view class="page_card_receipt_status" wx:else>
						已下架
					</view>
				</view>
				<view wx:if="{{$teamReceiptList$teamReceiptItem$receiptType==1}}">
					<view class="page_card_receipt_status {{$teamReceiptList$rItem.distributionnum?'page_card_receipt_status1':''}}">
						{{$teamReceiptList$rItem.distributionnum?'已分配':'未分配'}}
					</view>
				</view>
				<view wx:if="{{$teamReceiptList$teamReceiptItem$receiptType>1}}">
						<view wx:if="{{$teamReceiptList$teamReceiptItem$receiptType==2}}" class="page_card_receipt_status {{'page_card_receipt_status'+ (!$teamReceiptList$rItem.invoice_status?0:$teamReceiptList$rItem.invoice_status==1?1:2)}}">
							{{!$teamReceiptList$rItem.invoice_status?'收集中':$teamReceiptList$rItem.invoice_status==1?'审核中':'审核结束'}}
						</view>
						<view wx:if="{{$teamReceiptList$teamReceiptItem$receiptType==3&&!$teamReceiptList$rItem.interview_status}}" class="page_card_receipt_status page_card_receipt_status4">
							等待面试
						</view>
            <view wx:if="{{$teamReceiptList$teamReceiptItem$receiptType==3&&$teamReceiptList$rItem.interview_status}}" class="page_card_receipt_status {{'page_card_receipt_status'+ ($teamReceiptList$rItem.interview_status-1)}}">
							{{$teamReceiptList$rItem.interview_status==4?'面试结束':$teamReceiptList$rItem.interview_status==3?'面试审核中':'面试开始'}}
						</view>
						<view wx:if="{{$teamReceiptList$teamReceiptItem$receiptType==4&&!$teamReceiptList$rItem.entry_status}}" class="page_card_receipt_status page_card_receipt_status4">
						  等待入职
					  </view>
						<view wx:if="{{$teamReceiptList$teamReceiptItem$receiptType==4&&$teamReceiptList$rItem.entry_status}}" class="page_card_receipt_status {{'page_card_receipt_status'+ ($teamReceiptList$rItem.entry_status-1)}}">
						 {{$teamReceiptList$rItem.entry_status==4?'入职结束':$teamReceiptList$rItem.entry_status==3?'入职审核中':'入职开始'}}
				 	 </view>
				</view>
			</view>
			<view class="weui-flex between">
				<view class="weui-flex start weui-flex__item">
					{{$teamReceiptList$rItem.com_name}}
				</view>
				<view class="page_card_time" wx:if="{{$teamReceiptList$teamReceiptItem$receiptType<2}}">{{$teamReceiptList$rItem.addtime||$teamReceiptList$rItem.ctime}}</view>
				<view class="page_card_receipt_status page_card_receipt_status3" wx:if="{{$teamReceiptList$teamReceiptItem$receiptType==2&&!$teamReceiptList$rItem.view_time}}">
					未筛选简历
				</view>
				<view class="page_card_receipt_status page_card_receipt_status3" wx:elif="{{$teamReceiptList$teamReceiptItem$receiptType==3&&$teamReceiptList$rItem.interview_status<=1&&!$teamReceiptList$rItem.entry_time}}">
					未设置入职时间
				</view>
				<view wx:else>
					<view class="page_card_time" wx:if="{{$teamReceiptList$teamReceiptItem$receiptType==2}}">
						筛选通过 :
						{{$teamReceiptList$rItem.pass_num}}</view>
					<view class="page_card_time" wx:if="{{$teamReceiptList$teamReceiptItem$receiptType==3}}">
						{{$teamReceiptList$rItem.interview_status>2?'面试通过 : '+ $teamReceiptList$rItem.view_num:'筛选通过 : '+ $teamReceiptList$rItem.pass_num}}
					</view>
					<view class="page_card_time" wx:if="{{$teamReceiptList$teamReceiptItem$receiptType==4}}">
						{{$teamReceiptList$rItem.entry_status>2?'入职通过 : '+ $teamReceiptList$rItem.entry_num:'面试通过 : '+ $teamReceiptList$rItem.view_num}}</view>
				</view>
			</view>
		</view>
		<image mode="scaleToFill" class="page_down_icon" src="https://a.rsd123.com/image/images/right.png"/>
	</view>
	<!-- </view> -->

        </view>
      </block>
    </view>
    <view wx:else class="page_list">
      <image src="https://a.rsd123.com/image/images/noBg.png" class="page_noData" mode="scaleToFill"/>
      <view class="page_noData_text">暂无数据！</view>
    </view>
  </view>

				</view>
			</scroll-view>
		</view>
	</view>