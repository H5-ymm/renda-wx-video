
	<view class="home-view page_margin">
		<scroll-view scroll-y="true" bindscrolltolower="searchScrollLower" style="height: 100%;">
			
  <view>
    <block wx:for="{{$userSettingList$list}}" wx:for-index="$userSettingList$index" wx:for-item="$userSettingList$item" wx:key="$userSettingList$index">
      <view class="page_card"  wx:if="{{$userSettingList$list.length}}" bindtap="$userSettingList$settingRole" data-wpysettingrole-a="{{$userSettingList$item}}" data-wpysettingrole-b="{{$userSettingList$index}}">
        <view class="page_card_row weui-flex between">
          <view class="weui-flex__item">
            <view class="weui-flex between">
              <view class="page_card_name page_card_name1">{{$userSettingList$item.user_name?$userSettingList$item.user_name:$userSettingList$userName}}</view>
              <view class="page_card_name page_card_name1">
                {{$userSettingList$item.depart_name||'-'}}
              </view>
            </view>
            <view class="weui-flex between">
              <view class="page_card_col1 text-line">{{$userSettingList$item.mobile||'-'}}</view>
              <view class="page_card_time">{{$userSettingList$item.grade_name||'-'}}</view>
            </view>
          </view>
          <image mode="scaleToFill" wx:if="{{$userSettingList$item.uid!=$userSettingList$uid&&($userSettingList$item.grade_num==3||!$userSettingList$item.grade_num)}}" src="https://a.rsd123.com/image/images/down.png" class="page_down_icon {{$userSettingList$activeIndex==$userSettingList$index?'page_down_iconActive':''}}"/>
        </view>
      </view>
    </block>
    <view  wx:if="{{!$userSettingList$list.length}}" class="page_list">
      <image src="https://a.rsd123.com/image/images/noBg.png" class="page_noData" mode="scaleToFill"/>
      <view class="page_noData_text">暂无数据！</view>
    </view>
  </view>

		</scroll-view>
		<view class="picker"  wx:if="{{isPicker}}">
			<view class="weui-flex between picker-btn">
				<view class="weui-flex center" bindtap="selectClose">
					取消
				</view>
				<view class="weui-flex center ok-btn" bindtap="selectOk">
					确定
				</view>
			</view>
			<picker-view class="picker-view" indicator-style="height: 40px;" value="{{depIndex}}" bindchange="bindChange">
				<picker-view-column>
					<block wx:for="{{depList}}" wx:key="index">
						<view class="picker-view-item">{{item.depart_name}}</view>
					</block>
				</picker-view-column>
				<picker-view-column>
					<block wx:for="{{jobList}}" wx:key="index">
						<view class="picker-view-item">{{item.grade_name}}</view>
					</block>
				</picker-view-column>
			</picker-view>
		</view>
	</view>