
	<view class="detail-view">
		<view class="page_margin view-detail">
			<scroll-view scroll-y="true" style="height:100%">
				
	<view class="resume-list">
		<block  wx:for="{{$viewResultList$list}}" wx:key="index">
			<view class="page_card" wx:if="{{$viewResultList$list.length}}">
				<view class="page_card_row weui-flex between" bindtap="$viewResultList$viewDetail" data-wpyviewdetail-a="{{item}}">
					<view class="weui-flex start">
						<view>
							<view class="page_card_name">{{item.name}}</view>
							<view class="text-line weui-flex__item">
								{{item.age}}岁 {{item.sex==1?'男':'女'}} {{item.education}}</view>
						</view>
					</view>
					<view class="weui-flex end wrap page_card_end view-page_card">
							<view class="page_card_status-resume weui-flex end" wx:if="{{!item.interview_status}}">
								<button class="weui-btn_cell weui-flex__item weui-btn_primary" catchtap="$viewResultList$handleResume" data-wpyhandleresume-a="{{item}}" data-wpyhandleresume-b="5">
										待定
									</button>
									<button class="weui-btn_cell weui-flex__item weui-btn_primary_plain" catchtap="$viewResultList$handleResume" data-wpyhandleresume-a="{{item}}" data-wpyhandleresume-b="1">
										通过
									</button>
									<button class="weui-btn_cell weui-flex__item weui-btn_primary_plain" catchtap="$viewResultList$handleResume" data-wpyhandleresume-a="{{item}}" data-wpyhandleresume-b="2">
										未通过
									</button>
							</view>
							<view class="page_card_status-resume weui-flex end" wx:else>
								<view class="page_card_receipt_status {{'page_card_receipt_status' + item.interview_status}}">
									{{item.interview_status==1?'已通过':item.interview_status==2?'未通过':'待审核'}}
								</view>
							</view>
							<view class="page_card_time">{{item.addtime}}</view>
					</view>
				</view>
			</view>
		</block>
		<view  wx:if="{{!$viewResultList$list.length}}" class="page_list">
			<image src="https://a.rsd123.com/image/images/noBg.png" class="page_noData" mode="scaleToFill"/>
			<view class="page_noData_text">暂无数据！</view>
		</view>
	</view>

			</scroll-view>
		</view>
	</view>