
  <view class="detail-view record-view">
    <view class="weui-flex between header-box">
      <view class="date">
        {{dateValue?dateValue:'今日'}}
      </view>
      <picker mode="date" value="{{dateValue}}" bindchange="bindDateChange" class="modal-col picker-col">
        <view class="picker weui-flex end">
          <image src="https://a.rsd123.com/image/images/orderTaking/date.png" class="dateIcon"/>
        </view>
      </picker>
    </view>
    <view class="view-detail"  wx:if="{{list.length}}">
      <scroll-view scroll-y="true" style="height:100%">
        <block wx:for="{{list}}" wx:key="index">
          <view class="page_record">
            <view class="page_record-content">
              <view class="view-time">面试时间 : {{item.time}}</view>
              <view>面试内容 : {{item.content}}</view>
            </view>
            <block wx:for="{{item.data}}" wx:for-index="ind" wx:key="ind" wx:for-item="newitem">
              <view class="page_record_row" bindtap="viewInfo" data-wpyviewinfo-a="{{newitem}}">
                <view class="page_bottom page_record_col weui-flex start">
                  <view wx:if="{{receiptType==2}}">
                    <image src="https://a.rsd123.com/image/images/user/radio2.png" alt="" mode="aspectFill" class="radio-icon" catchtap="selectResume" wx:if="{{newitem.disabled&&!newitem.interview_status}}" data-wpyselectresume-a="{{item}}" data-wpyselectresume-b="{{index}}" data-wpyselectresume-c="{{newitem}}" data-wpyselectresume-d="{{ind}}"/>
                    <image src="https://a.rsd123.com/image/images/user/radio1.png" wx:if="{{!newitem.disabled&&!newitem.interview_status}}" catchtap="selectResume" mode="aspectFill" class="radio-icon" data-wpyselectresume-a="{{item}}" data-wpyselectresume-b="{{index}}" data-wpyselectresume-c="{{newitem}}" data-wpyselectresume-d="{{ind}}"/>
                  </view>
                  <view class="weui-flex between weui-flex__row">
                    <view class="weui-flex__info">
                      <view>{{newitem.name}}</view>
                      <view class="my-text">
                        {{newitem.sex==2?'女':'男'}} {{newitem.age}} {{newitem.eduName}}
                      </view>
                    </view>
                    <view class="page_card_receipt_status {{'page_card_receipt_status' + newitem.interview_status}} weui-flex__item" wx:if="{{newitem.interview_status}}">{{newitem.interview_status==1?'已通过':newitem.interview_status==2?'未通过':'待定'}}</view>
                  </view>
                </view>
              </view>
            </block>
            <view wx:if="{{receiptType==2&&!item.type}}" class="checkAll weui-flex start">
              <image src="https://a.rsd123.com/image/images/user/radio2.png" alt="" mode="aspectFill" class="radio-icon" catchtap="selectResume" wx:if="{{item.disabled}}" data-wpyselectresume-a="{{item}}" data-wpyselectresume-b="{{index}}" data-wpyselectresume-c="0" data-wpyselectresume-d="-1"/>
              <image src="https://a.rsd123.com/image/images/user/radio1.png" wx:else catchtap="selectResume" mode="aspectFill" class="radio-icon" data-wpyselectresume-a="{{item}}" data-wpyselectresume-b="{{index}}" data-wpyselectresume-c="0" data-wpyselectresume-d="-1"/>
              <text>全选</text>
            </view>
          </view>
        </block>
      </scroll-view>
    </view>
    <view  wx:else class="page_list">
      <image src="https://a.rsd123.com/image/images/noBg.png" class="page_noData" mode="scaleToFill"/>
      <view class="page_noData_text">暂无记录！</view>
    </view>
    <view class="weui-cell_btn weui-flex between register-btn" wx:if="{{receiptType==2&&list.length}}">
      <button class="weui-btn_cell weui-flex__item weui-btn_primary" bindtap="videoView">视频面试</button>
    </view>
  </view>