
	<view class="home-view">
		<view class="home-view-header"/>
		<view class="page page_margin">
			<scroll-view scroll-y="true" style="height:100%" bindscrolltolower="searchScrollLower">
				<view class="home-view-box">
					
  <view class="weui-flex between search-box weui-cell weui-flex start page_card">
		<view class="weui-cell weui-flex start weui-flex__item">
			<image src="../../images/search.png" class="searchIcon"/>
			<input class="weui-input weui-flex__item search-input" bindinput="$search$inputBind" confirm-type="search" bindfocus="$search$inputFocus" value="{{$search$name}}" placeholder="搜索词"/>
    </view>
		<text>搜索</text>
	</view>

					
  <view>
    <view  wx:if="{{$receiptAllList$list.length}}">
      <block wx:for="{{$receiptAllList$list}}" wx:for-index="$receiptAllList$index" wx:for-item="$receiptAllList$rItem" wx:key="$receiptAllList$index">
        <view class="page_card" bindtap="$receiptAllList$viewDetail" data-wpyviewdetail-a="{{$receiptAllList$rItem}}">
          <wxs src="../../filterWxs/receiptFilter.wxs" module="wxs"/>

	<view class="page_card_row weui-flex between">
		<view class="weui-flex__item">
			<view class="weui-flex between">
				<view class="page_card_name">
					<view>{{$receiptAllList$rItem.name||$receiptAllList$rItem.jobName}}</view>
					<view class="badgeNum badgeNum3"  wx:if="{{!$receiptAllList$rItem.com_isread&&!$receiptAllList$receiptItem$receiptType}}">
						New
					</view>
					<view  wx:if="{{$receiptAllList$rItem.noeReade&&$receiptAllList$receiptItem$receiptType}}">
						<view class="badgeNum" wx:if="{{$receiptAllList$rItem.noeReade<10}}">
							{{$receiptAllList$rItem.noeReade}}
						</view>
						<view class="badgeNum badgeNum1" wx:if="{{$receiptAllList$rItem.noeReade>10&&$receiptAllList$rItem.noeReade<100}}">{{$receiptAllList$rItem.noeReade}}</view>
						<view class="badgeNum badgeNum2" wx:if="{{$receiptAllList$rItem.noeReade>10&&$receiptAllList$rItem.noeReade>100}}">99+</view>
					</view>
				</view>
				<view wx:if="{{!$receiptAllList$receiptItem$receiptType}}">
					<view wx:if="{{$receiptAllList$rItem.job_status==1}}" class="page_card_receipt_status {{'page_card_receipt_status'+$receiptAllList$rItem.status}}">
						{{wxs.statusText($receiptAllList$rItem.status)}}
					</view>
					<view class="page_card_receipt_status page_card_receipt_status4" wx:else>
						已下架
					</view>
				</view>
				<view wx:if="{{$receiptAllList$receiptItem$receiptType}}">
					<!-- <view> -->
					<view wx:if="{{$receiptAllList$receiptItem$receiptType==1}}" class="page_card_receipt_status {{'page_card_receipt_status'+ (!$receiptAllList$rItem.invoice_status?0:$receiptAllList$rItem.invoice_status==1?1:2)}}">
						{{!$receiptAllList$rItem.invoice_status?'收集中':$receiptAllList$rItem.invoice_status==1?'审核中':'审核结束'}}
					</view>
					<view wx:if="{{$receiptAllList$receiptItem$receiptType==2}}" class="page_card_receipt_status {{'page_card_receipt_status'+(!$receiptAllList$rItem.interview_status?0:$receiptAllList$rItem.interview_status==1?1:2)}}">
						{{$receiptAllList$rItem.interview_status==4?'面试结束':'面试开始'}}
					</view>
					<view wx:if="{{$receiptAllList$receiptItem$receiptType==3}}" class="page_card_receipt_status {{'page_card_receipt_status'+($receiptAllList$rItem.entry_status==4?2:1)}}">
						{{$receiptAllList$rItem.entry_status!=4?'入职开始':'入职结束'}} </view>
				</view>
			</view>
			<view class="weui-flex between">
				<view class="weui-flex start weui-flex__item">
					需求人数：{{$receiptAllList$rItem.required_number}}
				</view>
				<view class="page_card_time" wx:if="{{!$receiptAllList$receiptItem$receiptType}}">{{$receiptAllList$rItem.addtime||$receiptAllList$rItem.ctime}}</view>
				<view class="page_card_receipt_status page_card_receipt_status3" wx:if="{{$receiptAllList$receiptItem$receiptType==1&!$receiptAllList$rItem.view_time&&!$receiptAllList$rItem.is_video}}">
					未筛选简历
				</view>
				<view class="page_card_receipt_status page_card_receipt_status3" wx:elif="{{$receiptAllList$receiptItem$receiptType==2&&$receiptAllList$rItem.interview_status<=1&&!$receiptAllList$rItem.entry_time}}">
					未设置入职时间
				</view>
				<view wx:else>
					<view class="page_card_time" wx:if="{{$receiptAllList$receiptItem$receiptType==1}}">
						筛选通过 :
						{{$receiptAllList$rItem.examineNum}}</view>
					<view class="page_card_time" wx:if="{{$receiptAllList$receiptItem$receiptType==2}}">
						{{$receiptAllList$rItem.interview_status>2?'面试通过 : '+ $receiptAllList$rItem.interviewNum:'筛选通过 : '+ $receiptAllList$rItem.examineNum}}
					</view>
					<view class="page_card_time" wx:if="{{$receiptAllList$receiptItem$receiptType==3}}">
						{{$receiptAllList$rItem.entry_status>2?'入职通过 : '+ $receiptAllList$rItem.entryNum:'面试通过 : '+ $receiptAllList$rItem.interviewNum}}</view>
				</view>
			</view>
		</view>
		<image mode="scaleToFill" class="page_down_icon" src="https://a.rsd123.com/image/images/right.png"/>
	</view>

        </view>
      </block>
    </view>
    <view wx:else class="page_list">
      <image src="https://a.rsd123.com/image/images/noBg.png" class="page_noData" mode="scaleToFill"/>
      <view class="page_noData_text">暂无数据！</view>
    </view>
  </view>

				</view>
			</scroll-view>
		</view>
		
	<view hidden="{{$noticeModal$isScaleModal}}">
		<view bindtap="$noticeModal$ handleClose"  wx:if="{{$noticeModal$isShow}}" class="modal_mask"/>
		<view class="modal_box modal-notice_box transition3s {{ $noticeModal$isShow ? 'g_scale1': ''}}" style="height:{{$noticeModal$height}}rpx">
			<view class="modal-content">
				<view class="modal-title">设置{{$noticeModal$title}}通知</view>
				<view class="modal-view-box {{$noticeModal$type==2?'modal-view-box1':''}}">
					<view class="modal-row">
						<text>{{$noticeModal$title}}时间</text>
						
	<view class="weui-cell weui-flex between">
		<picker mode="date" value="{{$noticeModal$datePicker$dateValue}}" start="{{$noticeModal$datePicker$startValue}}" bindchange="$noticeModal$datePicker$bindDateChange" class="modal-col picker-col">
			<view class="picker weui-flex between">
				<view class="weui-flex between weui-flex__item">
					<view class="{{!$noticeModal$datePicker$params.date?'placeholder':''}}">{{$noticeModal$datePicker$dateValue}}</view>
					<image src="https://a.rsd123.com/image/images/orderTaking/date.png" class="dateIcon"/>
				</view>
			</view>
		</picker>
		<picker mode="time" value="{{$noticeModal$datePicker$timeValue}}" start="09:00" end="21:00" bindchange="$noticeModal$datePicker$timeChange" class="modal-col picker-col">
			<view class="picker weui-flex between">
				<view class="weui-flex between weui-flex__item">
					<view class="{{!$noticeModal$datePicker$params.time?'placeholder':''}}">{{$noticeModal$datePicker$timeValue}}</view>
					<image src="https://a.rsd123.com/image/images/orderTaking/time.png" class="dateIcon"/>
				</view>
			</view>
		</picker>
	</view>

					</view>
					<view class="modal-row"  wx:if="{{$noticeModal$type==1}}">
						<text>{{$noticeModal$title}}地址</text>
						
  <picker mode="multiSelector" bindcancel="$noticeModal$districtSelet$cancelPickerChange" bindchange="$noticeModal$districtSelet$bindMultiPickerChange" bindcolumnchange="$noticeModal$districtSelet$bindMultiPickerColumnChange" value="{{$noticeModal$districtSelet$multiIndex}}" range="{{$noticeModal$districtSelet$multiArray}}" class="modal-col">
    <view class="my-text-right weui-flex between">
      <view  wx:if="{{$noticeModal$districtSelet$options.length}}">
          {{$noticeModal$districtSelet$multiArray[0][$noticeModal$districtSelet$multiIndex[0]]}}
          {{$noticeModal$districtSelet$multiArray[1][$noticeModal$districtSelet$multiIndex[1]]}}
          {{$noticeModal$districtSelet$disabled?$noticeModal$districtSelet$multiArray[2][$noticeModal$districtSelet$multiIndex[2]]:''}}
       </view>
      <view  wx:else class="placeholder">请选择地址</view>
      <image mode="scaleToFill" src="https://a.rsd123.com/image/images/down.png" class="page_down_icon"/>
    </view>
  </picker>

						<view class="modal-col modal-col-input">
							<input class="weui-input" placeholder-class="placeholder" data-name="address" value="{{$noticeModal$form.address}}" bindinput="$noticeModal$changeInput" placeholder="请输入详细地址"/>
						</view>
					</view>
					<view class="modal-row">
						<text>通知内容</text>
						<textarea data-name="content" placeholder-class="placeholder" disable-default-padding="true" class="weui-input weui-textarea modal-col" value="{{$noticeModal$form.content}}" bindinput="$noticeModal$changeInput" placeholder="请输入{{$noticeModal$title}}通知的内容" maxlength="-1"/>
						</view>
					<view class="weui-flex start notice-tip" wx:if="{{$noticeModal$type==1}}">
						<image src="https://a.rsd123.com/image/images/orderTaking/warn.png" class="tipIcon"/>
						<text class="tip">审核通过简历会直接发送短信通知{{$noticeModal$title}}，谨慎操作</text>
					</view>
					<view class="weui-flex start notice-tip" wx:else>
						<image src="https://a.rsd123.com/image/images/orderTaking/warn.png" class="tipIcon"/>
					 <view>
						 	<text class="tip">1.筛选通过简历直接短信通知视频面试信息，谨慎 操作</text>
						<view class="tip">2.可根据实际情况修改面试通知，分批面试</view>
					 </view>
					</view>
		    </view>
        <view class="weui-cell_btn weui-flex between create-btn">
          <button class="weui-btn_cell weui-btn_primary_plain" bindtap="$noticeModal$handleClose">取消</button>
					<button class="weui-btn_cell weui-btn_primary" bindtap="$noticeModal$handleOk">{{$noticeModal$timeInfo&&$noticeModal$type==1?'修改':'确定'}}</button>
        </view>
      </view>
      <image src="https://a.rsd123.com/image/images/close.png" bindtap="$noticeModal$handleClose" class="close-icon"/>
    </view>
  </view>

		
	<view hidden="{{$selectModal$isScaleModal}}">
		<view bindtap="$selectModal$ handleClose"  wx:if="{{$selectModal$isShow}}" class="modal_mask"/>
		<view class="modal_box transition3s {{ $selectModal$isShow ? 'g_scale1': ''}}" id="modal-box" style="height:{{$selectModal$height}}rpx">
			<view class="modal-content">
				<view class="home-view-box weui-flex between wrap">
					<view class="modal-title ">{{$selectModal$title}} </view>
					<block  wx:for="{{$selectModal$menus}}" wx:key="index">
						<view class="card-item weui-flex between wrap" bindtap="$selectModal$selectType" data-wpyselecttype-a="{{index}}">
							<image src="{{item.icon}}" alt="" mode="aspectFill" class="menu-icon"/>
							<view class="card-item-title">{{item.title}}</view>
							<image src="https://a.rsd123.com/image/images/user/radio2.png" alt="" mode="aspectFill" wx:if="{{index==$selectModal$activeIndex}}" class="radio-icon"/>
							<image src="https://a.rsd123.com/image/images/user/radio1.png" wx:else alt="" mode="aspectFill" class="radio-icon"/>
						</view>
					</block>
				</view>
				<view class="weui-cell_btn weui-flex center footer-btn">
					<button class="weui-btn_cell weui-btn_cell-gradient" bindtap="$selectModal$handleOk">确定</button>
				</view>
				<view>
				<view class="weui-flex center notice-tip" wx:if="{{$selectModal$menus[0].title=='正常面试'}}">
					<image src="https://a.rsd123.com/image/images/orderTaking/warn.png" class="tipIcon"/>
					<text class="tip">通过简历根据已选择面试类型分类</text>
				</view>
				</view>
			</view>
			<image src="https://a.rsd123.com/image/images/close.png" bindtap="$selectModal$handleClose" class="close-icon"/>
		</view>
	</view>

	</view>