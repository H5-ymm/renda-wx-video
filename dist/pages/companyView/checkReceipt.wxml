
	<view class="home-view">
		<view class="home-view-header"/>
		<view class="page page_margin">
			<scroll-view scroll-y="true" style="height:100%" bindscrolltolower="searchScrollLower">
				<view class="home-view-box">
					
  <view class="weui-flex between search-box page_card">
		<view class="weui-cell weui-flex start weui-flex__item">
			<image src="../../images/search.png" class="searchIcon"/>
			<input class="weui-input weui-flex__item search-input" bindinput="$search$inputBind" confirm-type="search" bindfocus="$search$inputFocus" value="{{$search$name}}" placeholder="搜索词"/>
    </view>
		<text>搜索</text>
	</view>

					
  <view>
    <view  wx:if="{{$receiptList$list.length}}">
      <block wx:for="{{$receiptList$list}}" wx:for-index="$receiptList$index" wx:for-item="$receiptList$rItem" wx:key="$receiptList$index">
        <view class="page_card" bindtap="$receiptList$viewDetail" data-wpyviewdetail-a="{{$receiptList$rItem}}">
          <wxs src="../../filterWxs/receiptFilter.wxs" module="wxs"/>

	<view class="page_card_row weui-flex between">
		<view class="weui-flex__item">
			<view class="weui-flex between">
				<view class="page_card_name">
					<view>{{$receiptList$rItem.name||$receiptList$rItem.jobName}}</view>
					<view class="badgeNum badgeNum3"  wx:if="{{!$receiptList$rItem.com_isread&&!$receiptList$receiptItem$receiptType}}">
						New
					</view>
					<view  wx:if="{{$receiptList$rItem.noeReade&&$receiptList$receiptItem$receiptType}}">
						<view class="badgeNum" wx:if="{{$receiptList$rItem.noeReade<10}}">
							{{$receiptList$rItem.noeReade}}
						</view>
						<view class="badgeNum badgeNum1" wx:if="{{$receiptList$rItem.noeReade>10&&$receiptList$rItem.noeReade<100}}">{{$receiptList$rItem.noeReade}}</view>
						<view class="badgeNum badgeNum2" wx:if="{{$receiptList$rItem.noeReade>10&&$receiptList$rItem.noeReade>100}}">99+</view>
					</view>
				</view>
				<view wx:if="{{!$receiptList$receiptItem$receiptType}}">
					<view wx:if="{{$receiptList$rItem.job_status==1}}" class="page_card_receipt_status {{'page_card_receipt_status'+$receiptList$rItem.status}}">
						{{wxs.statusText($receiptList$rItem.status)}}
					</view>
					<view class="page_card_receipt_status page_card_receipt_status4" wx:else>
						已下架
					</view>
				</view>
				<view wx:if="{{$receiptList$receiptItem$receiptType}}">
					<!-- <view> -->
					<view wx:if="{{$receiptList$receiptItem$receiptType==1}}" class="page_card_receipt_status {{'page_card_receipt_status'+ (!$receiptList$rItem.invoice_status?0:$receiptList$rItem.invoice_status==1?1:2)}}">
						{{!$receiptList$rItem.invoice_status?'收集中':$receiptList$rItem.invoice_status==1?'审核中':'审核结束'}}
					</view>
					<view wx:if="{{$receiptList$receiptItem$receiptType==2}}" class="page_card_receipt_status {{'page_card_receipt_status'+(!$receiptList$rItem.interview_status?0:$receiptList$rItem.interview_status==1?1:2)}}">
						{{$receiptList$rItem.interview_status==4?'面试结束':'面试开始'}}
					</view>
					<view wx:if="{{$receiptList$receiptItem$receiptType==3}}" class="page_card_receipt_status {{'page_card_receipt_status'+($receiptList$rItem.entry_status==4?2:1)}}">
						{{$receiptList$rItem.entry_status!=4?'入职开始':'入职结束'}} </view>
				</view>
			</view>
			<view class="weui-flex between">
				<view class="weui-flex start weui-flex__item">
					需求人数：{{$receiptList$rItem.required_number}}
				</view>
				<view class="page_card_time" wx:if="{{!$receiptList$receiptItem$receiptType}}">{{$receiptList$rItem.addtime||$receiptList$rItem.ctime}}</view>
				<view class="page_card_receipt_status page_card_receipt_status3" wx:if="{{$receiptList$receiptItem$receiptType==1&!$receiptList$rItem.view_time&&!$receiptList$rItem.is_video}}">
					未筛选简历
				</view>
				<view class="page_card_receipt_status page_card_receipt_status3" wx:elif="{{$receiptList$receiptItem$receiptType==2&&$receiptList$rItem.interview_status<=1&&!$receiptList$rItem.entry_time}}">
					未设置入职时间
				</view>
				<view wx:else>
					<view class="page_card_time" wx:if="{{$receiptList$receiptItem$receiptType==1}}">
						筛选通过 :
						{{$receiptList$rItem.examineNum}}</view>
					<view class="page_card_time" wx:if="{{$receiptList$receiptItem$receiptType==2}}">
						{{$receiptList$rItem.interview_status>2?'面试通过 : '+ $receiptList$rItem.interviewNum:'筛选通过 : '+ $receiptList$rItem.examineNum}}
					</view>
					<view class="page_card_time" wx:if="{{$receiptList$receiptItem$receiptType==3}}">
						{{$receiptList$rItem.entry_status>2?'入职通过 : '+ $receiptList$rItem.entryNum:'面试通过 : '+ $receiptList$rItem.interviewNum}}</view>
				</view>
			</view>
		</view>
		<image mode="scaleToFill" class="page_down_icon" src="https://a.rsd123.com/image/images/right.png"/>
	</view>

        </view>
      </block>
    </view>
    <view wx:else class="page_list">
      <image src="https://a.rsd123.com/image/images/noBg.png" class="page_noData" mode="scaleToFill"/>
      <view class="page_noData_text">暂无数据！</view>
    </view>
  </view>

				</view>
			</scroll-view>
		</view>
	</view>