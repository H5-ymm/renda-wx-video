  .choose
  {
    display: flex;
    flex-direction: column;
    width :100%;
    height: 100%;
  }
.title
 {
    height: 70%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-size: 32px;
  font-weight: 700;
 }
.btn
  {
    height: 30%;
  width: 100%;
  justify-content: space-around;
  display :flex;
  }
.close
  {
    background-color :red;
    height :50px;
    width :50px;
    padding: 12px;
    box-sizing:border-box;
    border-radius:50%;
 }
.answer
 {
    background-color:green;
 }
.call
  {
    display: flex;
  position: relative;
  }
.close-btn
  {
    z-index: 999;
  display: flex;
  width: 100%;
  justify-content: space-around;
  box-sizing: border-box;
  }
.time
{
    height: 8%;
  width: 100%;
  text-align: center;
  padding-top :30px;
  color :white;
}
.panel
{
    z-index: 999;
  display: flex;
  width: 100%;
  bottom :20px;
  flex-direction: column;
  box-sizing: border-box;
  position :absolute;
}
.room
 {
    position :relative;
  height: 100%;
  width :100%;
 }
.operation
  {
    width :100%;
  height: 100%;
  }
.normal
  {
    background-color: transparent;
    height: 50px;
    width :50px;
    padding :10px;
    box-sizing :border-box;
    border :2px solid white;
    border-radius: 50%;
 }
 .isCalling2 {
   display: flex;height: 100%; width: 100%;
 }
 .isCalling1 {
   display: none;
 }