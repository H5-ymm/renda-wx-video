
 <view class="page page_top resume_view view_hidden">
    <scroll-view scroll-y="true" bindscrolltolower="searchScrollLower" class="view-content1">
      
	<view class="scroll-view page">
	  <block  wx:if="{{$jobManageList$list.length}}" wx:for="{{$jobManageList$list}}" wx:for-index="$jobManageList$index" wx:for-item="$jobManageList$item" wx:key="$jobManageList$index">
      <view bindtap="$jobManageList$viewJobDetail" data-wpyviewjobdetail-a="{{$jobManageList$item}}">
				
  <view class="list-row weui-flex between" bindtap="$jobManageList$jobManageItem$selectJob" data-com-index="{{$jobManageList$index}}" data-wpyselectjob-a="{{$jobManageList$jobManageItem$index}}" data-wpyselectjob-b="{{$jobManageList$item}}">
    <view class="list-col-1 weui-flex__item">
      <view class="weui-flex between weui-flex__item">
        <view class="page__title">{{$jobManageList$item.job_name}}</view>
        <view class="page__desc">待审核</view>
      </view>
      <view class="number">{{$jobManageList$item.salary}}</view>
      <view class="page__desc">全职｜{{$jobManageList$item.edu}}｜{{$jobManageList$item.province}}{{$jobManageList$item.city}}{{$jobManageList$item.area}}</view>
    </view>
    <view class="weui-flex between">
      <image mode="scaleToFill" class="page_card_icon" src="https://a.rsd123.com/image/images/right.png"/>
    </view>
  </view>

			</view>
    </block>
		<view  wx:if="{{!$jobManageList$list.length}}" class="page_list">
			<image src="../../images/noData.png" class="page_noData" mode="scaleToFill"/>
			<view class="page_noData_text">暂无数据！</view>
		</view>
	</view>

    </scroll-view>
    <view class="weui-cell_btn weui-flex center">
      <button class="weui-btn_cell weui-btn_primary" bindtap="releaseJob">发布职位</button>
    </view>
  </view>