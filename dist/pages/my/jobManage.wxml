
 <view class="page page_top resume_view view_hidden">
    <scroll-view scroll-y="true" bindscrolltolower="searchScrollLower" class="view-content1">
      
	<view class="scroll-view page">
	  <block  wx:if="{{$jobManageList$list.length}}" wx:for="{{$jobManageList$list}}" wx:for-index="$jobManageList$index" wx:for-item="$jobManageList$item" wx:key="$jobManageList$index">
      <view bindtap="$jobManageList$viewJobDetail" data-wpyviewjobdetail-a="{{$jobManageList$item}}">
				
  <view class="list-row weui-flex between _78f4484">
    <view class="list-col-1 weui-flex__item _78f4484">
      <view class="weui-flex between weui-flex__item _78f4484">
        <view class="page__title _78f4484">{{$jobManageList$item.job_name}}</view>
        <view class="page__desc _78f4484"  wx:if="{{!$jobManageList$item.status}}">待审核</view>
        <view class="page__desc {{$jobManageList$item.is_up==1?'status2':'status4'}} _78f4484"  wx:if="{{$jobManageList$item.status == 1}}">{{$jobManageList$item.is_up==1?'招聘中':'已结束'}}</view>
        <view class="page__desc status3 _78f4484"  wx:if="{{$jobManageList$item.status == 2}}">已拒绝</view>
      </view>
      <view class="number _78f4484">{{$jobManageList$item.salary}}</view>
      <view class="page__desc _78f4484">{{$jobManageList$item.nature==3?'实习':$jobManageList$item.nature==2?'兼职':'全职'}}｜{{$jobManageList$item.edu}}｜{{$jobManageList$item.province}}{{$jobManageList$item.city}}{{$jobManageList$item.area}}</view>
    </view>
    <view class="weui-flex between _78f4484">
      <image mode="scaleToFill" class="page_card_icon _78f4484" src="http://www.ttxsg.com.cn:39009/uploads/images/right.png"/>
    </view>
  </view>
			</view>
    </block>
		<view wx:if="{{!$jobManageList$list.length}}" class="page_list">
			<image src="http://www.ttxsg.com.cn:39009/uploads/images/noData.png" class="page_noData" mode="scaleToFill"/>
			<view class="page_noData_text">暂无数据！</view>
		</view>
	</view>

    </scroll-view>
    <view class="weui-cell_btn weui-flex center">
      <button class="weui-btn_cell weui-btn_primary" bindtap="releaseJob">发布职位</button>
    </view>
  </view>