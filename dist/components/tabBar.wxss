.tabBar{height:100rpx;color:#111;font-size:32rpx;position:relative;line-height:32rpx;width:80%;margin:0 auto}.tabBar .tab-line{position:absolute;left:10%;bottom:20%;background:#1890ff;height:4rpx;width:80rpx;transition:.7s;border-radius:4rpx}.tabBar .tab-line.line1{left:44%}.tabBar .tab-line.line2{left:78%}.tabBar .tabBar_item{text-align:center;flex:auto}.tabBar .tabBar_item.tabBar_item-active{color:#1890ff}